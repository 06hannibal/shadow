<?php

/**
 * Implements hook_theme() to add the template definition.
 **/

function manufacture_marketplace_theme($existing, $type, $theme, $path) {
  $theme = [];
  $theme['manufacture_marketplace_list_template'] = [
    'template' => 'manufacture-marketplace-list-template',
    'variables' => [
      'product_list' => NULL,
    ],
  ];
  return $theme;
}

